{
    "name" : "vcspace_dark",
    "settings" : [ {
        "settings" : {
            "foreground" : "#d4d9d6",
            "lineHighlight" : "#2B2B2B",
            "selection" : "#214283",
            "lineNumber" : "#b3b4b5",
            "currentLineNumber" : "#d4d9d6",
            "highlightedDelimetersForeground" : "#ffffff",
            "caret": "#d4d9d6"
